({type: 'FETCH_SUCCESS', show:results });\r\n            \r\n            }\r\n            \r\n        })\r\n        .catch(err=>{\r\n            if(isMounted){\r\n                dispatch({type: 'FETCH_FAILED', error:err.message });\r\n            }\r\n        });\r\n\r\n        return()=>{\r\n        isMounted = false;\r\n        };\r\n    },[id]);\r\n\r\n    console.log(\"show\",show);\r\n\r\n\r\n    if(isLoading){\r\n        return <div>Data being loaded</div>;\r\n    }\r\n\r\n    if(error){\r\n        return <div>Error occured:{error}</div>;\r\n    }\r\n\r\n    \r\n    return (\r\n        <div>\r\n         <ShowMainData \r\n         image = {show.image} \r\n         name = {show.name}\r\n         rating = {show.rating}\r\n         summary = {show.summary}\r\n         tags = {show.genres} />\r\n\r\n         <div>\r\n             <h2>Details</h2>\r\n             <Details\r\n             status = {show.status}\r\n             network = {show.network} \r\n             premiered = {show.premiered} />\r\n        </div> \r\n\r\n        <div>\r\n             <h2>Seasons</h2>\r\n             <Seasons \r\n             season = {show._embedded.seasons}  />\r\n        </div> \r\n\r\n        <div>\r\n             <h2>Cast</h2>\r\n             <Cast cast = {show._embedded.cast}/>\r\n        </div> \r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Show\r\n","D:\\React Training From Internshala\\boxofficeApp\\boxofficeapp\\src\\components\\show\\Details.js",[],"D:\\React Training From Internshala\\boxofficeApp\\boxofficeapp\\src\\components\\show\\ShowMainData.js",[],"D:\\React Training From Internshala\\boxofficeApp\\boxofficeapp\\src\\components\\show\\Seasons.js",[],"D:\\React Training From Internshala\\boxofficeApp\\boxofficeapp\\src\\components\\show\\Cast.js",[],{"ruleId":"104","replacedBy":"105"},{"ruleId":"106","replacedBy":"107"},{"ruleId":"108","replacedBy":"109"},{"ruleId":"110","replacedBy":"111"},{"ruleId":"112","replacedBy":"113"},{"ruleId":"104","replacedBy"